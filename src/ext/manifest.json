{
    "manifest_version": 2,
    "name": "Snapper",
    "short_name": "Snapper",
    "version": "1.0",
    "description": "",
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": false
    }],
    "permissions":["tabs","pageCapture","webNavigation","<all_urls>"],
    "background": {
      "scripts": ["background.js"]
    },
    "icons": { "128": "icon128.png" }
  }
